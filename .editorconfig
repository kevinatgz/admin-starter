# https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
end_of_line = lf
insert_final_newline = false
trim_trailing_whitespace = true

[*.js]
indent_size = 4


[*.md]
insert_final_newline = false
trim_trailing_whitespace = false

[*.yml]
indent_style = space
indent_size = 2

[*.java]
indent_style = space
indent_size = 4

[{package.json,.travis.yml}]
indent_style = space
indent_size = 2
